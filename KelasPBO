public class KelasPBO extends Acara {
    private String materi;

    public KelasPBO(String nama, String lokasi, String tanggal, String waktu, String materi) {
        super(nama, lokasi, tanggal, waktu);
        this.materi = materi;
    }

    public String getMateri() {
        return materi;
    }

    @Override
    public void tampilkanDetailAcara() {
        super.tampilkanDetailAcara();
        System.out.println("Materi: " + materi);
    }
}
